$(".flash_notice").empty()
$(".flash_notice").append("<%= escape_javascript(flash.discard(:notice))%>");
$("#form_holder").empty();
$("#form_holder").append("<%= escape_javascript(render(:partial => "start_tracking", :locals => {:log => current_user.logs.build })) %>");
$("#log_<%= escape_javascript(@log.id.to_s) %>").remove();
$("#head_log").after("<%= escape_javascript(render(:partial => @log)) %>");
$("#form_holder").find("input#log_event").val('');

$(".open_tracking_select").UIdialogs_tracking_logs_links();
$(".open_log_update").UIdialogs_edit_logs_links();
$(".start_tracking_form").tracking_logs_WithAjax();
$(".spinning").hide();
