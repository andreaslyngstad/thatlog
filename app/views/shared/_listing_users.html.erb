<% @users.each do |user| %>
  <li>
    <span>   
    <%= link_to image_tag("gears.png", :border=>0), edit_user_path(user), :title => "Edit user" %>
    <%= link_to image_tag("user-delete.png", :border=>0), user_path(user), :title => "Delete user", :confirm => 'Deleting ' + "#{user.name}""."+ " Are you sure?", :method => :delete %>
  </span>
    
  </li>
<% end %>



