var object = "<%= escape_javascript(@model) %>"

$("#dialog_" + object + "_form").UIdialogs();
$("#dialog_" + object + "_form").children(".new_" + object).validateWithErrors();
$("#dialog_" + object + "_form").dialog( "open" );

$("#dialog_" + object + "_form").prepend("<%= escape_javascript( render :partial => "shared/validation_error", :locals => { :object => @model_instanse })  %>");
$(".spinning").hide();
