$(".flash_notice").empty()
$(".flash_notice").append("<%= escape_javascript(flash.discard(:notice))%>")
<% if @project.users.include?(@user) %>
$(".users_count").text(parseInt($(".users_count").text()) + 1)
<% else %>
$(".users_count").text(parseInt($(".users_count").text()) - 1)
<% end %>
$(".spinning").hide();